        {
            "name": "ns-train (WAT-breville)",
            "type": "debugpy",
            "request": "launch",
            "program": "nerfstudio/scripts/train.py",
            "console": "integratedTerminal",
            "env":
            {
                "CUDA_VISIBLE_DEVICES": "0",
                "CUDA_LAUNCH_BLOCKING": "1",
            },
            "args": [
                "splatfacto-big",
                "--data",
